        <TextBlock x:Name="randomText" HorizontalAlignment="Center" VerticalAlignment="Center" Height="40" Width="780" FontSize="20" TextAlignment="Center"></TextBlock>
        <Button HorizontalAlignment="Center" VerticalAlignment="Bottom" Height="50" Width="200" FontSize="20" Click="Button_Click">Button</Button>
    </Grid>
